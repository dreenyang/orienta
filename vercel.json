{
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/index.html"
    },
    {
      "source": "/index",
      "destination": "/pages/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/pages/index.html"
    },
    {
      "source": "/about",
      "destination": "/pages/about.html"
    },
    {
      "source": "/about.html",
      "destination": "/pages/about.html"
    },
    {
      "source": "/contact",
      "destination": "/pages/contact.html"
    },
    {
      "source": "/contact.html",
      "destination": "/pages/contact.html"
    },
    {
      "source": "/services",
      "destination": "/pages/services.html"
    },
    {
      "source": "/services.html",
      "destination": "/pages/services.html"
    },
    {
      "source": "/resources",
      "destination": "/pages/resources.html"
    },
    {
      "source": "/resources.html",
      "destination": "/pages/resources.html"
    },
    {
      "source": "/how-it-works",
      "destination": "/pages/how-it-works.html"
    },
    {
      "source": "/how-it-works.html",
      "destination": "/pages/how-it-works.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}